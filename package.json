    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "0.44.4",
    "next": "^15.0.0",
    "postgres": "^3.4.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "semantic-release": "^24.2.7",
    "short-uuid": "^5.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "zod": "3.25.76"
  },
  "peerDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "ai": "5.0.0",
    "better-auth": "^1.3.4",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "0.44.4",
    "next": "^15.0.0",
    "pino": "^9.6.0",
    "postgres": "^3.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "short-uuid": "^5.2.0",
    "validator": "^13.15.0",
    "zod": "^3.25.76"
  },
  "config": {
    "commitizen": {
